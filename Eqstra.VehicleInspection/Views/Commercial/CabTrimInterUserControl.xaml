                                Grid.Column="2"
                                Grid.Row="1"
                                Style="{StaticResource TransparentBtn}">
                            <Image Source="ms-appx:///Assets/camera_icon.png"
                                   Grid.Row="0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   Width="28"
                                   Height="28" />
