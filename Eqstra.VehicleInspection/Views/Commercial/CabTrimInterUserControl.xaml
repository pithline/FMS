    <UserControl.Resources>
        <Converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <c:CommandParameterConverter x:Key="CPConverter" />
    </UserControl.Resources>
    <Grid HorizontalAlignment="Stretch"
          MinWidth="800">
        <Grid Grid.Column="0"
              Margin="0 0 0 0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <StackPanel Orientation="Horizontal">
                <TextBlock Text="Cab/Trim/Interior"
                           Style="{StaticResource GroupHeaderStyle}"
                           VerticalAlignment="Bottom"
                           Margin="0 0 0 5" />
                <Button Style="{StaticResource TransparentBtn}"
                        Margin="10 0 0 0"
                        VerticalAlignment="Center"
                        IsEnabled="{Binding Model.ShouldSave}"
                        Command="{Binding SaveModelCommand}"
                        CommandParameter="{Binding Model}">
                    <Image Source="ms-appx:///Assets/save.png" />
                </Button>
            </StackPanel>

            <Border Grid.Row="1"
                    BorderBrush="#FFE0E0E0"
                    BorderThickness="1"
                    Background="White"
                    Margin="0"
                    HorizontalAlignment="Stretch">
                <ScrollViewer ZoomMode="Disabled">

