                                       OnContent="Yes"
                                       x:Name="LeftDoorToggle"
                                       IsOn="{Binding Path=Model.IsLeftDoorDmg ,Mode=TwoWay}"
                                       Grid.Row="1"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Margin="5 0" />
