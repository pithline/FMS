                                  Grid.Column="4"
                                  Height="45"
                                  Width="140"
                                  Padding="0 0 0 0"
                                  HorizontalAlignment="Left"
                                  SelectionMode="None">
                            <GridView.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <VirtualizingStackPanel Orientation="Horizontal" />
                                </ItemsPanelTemplate>
                            </GridView.ItemsPanel>
                            <GridView.ItemTemplate>
                                <DataTemplate>
                                    <Image Source="{Binding ImagePath}"
